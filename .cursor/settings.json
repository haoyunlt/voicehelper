{
  "cursor.ai.maxTokens": 4000,
  "cursor.ai.temperature": 0.1,
  "cursor.ai.model": "claude-3.5-sonnet",
  "cursor.ai.enableCodeContext": true,
  "cursor.ai.contextWindow": "medium",
  "cursor.ai.enableFileSearch": false,
  "cursor.ai.enableSymbolSearch": true,
  "cursor.ai.maxFileSize": 100000,
  "cursor.ai.excludePatterns": [
    "**/__pycache__/**",
    "**/*.pyc",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.git/**",
    "**/logs/**",
    "**/data/**",
    "**/models/**",
    "**/cache/**"
  ],
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.semanticHighlighting.enabled": false,
  "editor.codeLens": false,
  "editor.inlayHints.enabled": "off",
  "editor.suggest.maxVisibleSuggestions": 5,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/__pycache__/**": true,
    "**/*.pyc": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/logs/**": true,
    "**/data/**": true,
    "**/models/**": true,
    "**/cache/**": true,
    "**/.pytest_cache/**": true,
    "**/.coverage": true,
    "**/htmlcov/**": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/__pycache__": true,
    "**/dist": true,
    "**/build": true,
    "**/logs": true,
    "**/data": true,
    "**/models": true,
    "**/cache": true
  },
  "files.associations": {
    "*.py": "python",
    "*.go": "go",
    "*.ts": "typescript",
    "*.tsx": "typescriptreact"
  },
  "python.analysis.autoImportCompletions": false,
  "python.analysis.indexing": false,
  "go.useLanguageServer": true,
  "go.languageServerFlags": [
    "-rpc.trace"
  ],
  "typescript.suggest.autoImports": "off",
  "typescript.preferences.includePackageJsonAutoImports": "off"
}